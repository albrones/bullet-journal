{
  "compilerOptions": {
    "target": "ES5",
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "jsx": "preserve"
  },
  "exclude": [
    "node_modules",
    "wwwroot",
    "packages",
    "typings"
  ]
}